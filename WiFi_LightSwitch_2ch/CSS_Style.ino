 String Create_CSS (void){

String out = "";

out +=  ".img-center{ display: block; margin: 0 auto; width: 262px; height: 49px;}";
out +=  ".navig{background-color: lightgray; text-align: center;font-size: 16pt;word-spacing: 40px;}";
out +=  ".heder-size{font-size: 18pt; font-family: Georgia, serif;color: darkcyan; font-style: oblique; font-weight: bold;}";
out +=  ".slogan{font-size: 10pt;color: gray; font-style: italic;}";
out +=  ".mode{text-align: center; margin-top: 20px; margin-bottom: 5px;font-weight: bold;}";
out +=  ".mode_ON{text-align: center; width: 98px; color:white ; padding: 10px 30px; background-color: #43a209; margin: 0 auto;}";
out +=  ".mode_OFF{text-align: center; width: 98px; color:white; padding: 10px 30px; background-color: red; margin: 0 auto;}";
out +=  "a{text-decoration: none; color: black;}";
out +=  "button{text-align: center; margin-left: auto; margin-right: auto; height: 40px; width: 70px; font-weight: bold;}";
out +=  ".button_mode{text-align: center; margin: 5px 0px;}";
out +=  ".button_refresh{text-align: center; margin-top: 20px;}";
out +=  ".button_submit{margin-top: 20px; text-align: center;}";
out +=  ".field {clear:both; text-align: right; line-height:25px;}";
out +=  "label {float:left; padding-right:10px; font-weight: bold;}";
out +=  ".main {float:left;}";
return(out);
}
